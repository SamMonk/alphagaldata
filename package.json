{
  "name": "alphagaldata",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "gatsby develop",
    "prepare:data": "node scripts/sync-ingredients.js",
    "build": "npm run prepare:data && gatsby build",
    "serve": "gatsby serve",
    "deploy:s3": "S3_BUCKET=alphagal S3_REGION=us-east-1 ./scripts/deploy_s3.sh",
    "deploy:s3:plugin": "npm run build && gatsby-plugin-s3 deploy --yes",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "tasks:cards": "node scripts/generate_cards.js",
    "tasks:pubmed": "node scripts/fetch_pubmed.js",
    "tasks:cdc": "node scripts/fetch_cdc_ticks.js",
    "tasks:xlsx": "node scripts/convert_xlsx_to_geojson.js",
    "tasks:counties": "node scripts/prepare_counties.js",
    "tasks:map": "node scripts/build_map_from_xlsx.js"
  },
  "dependencies": {
    "gatsby": "^5.13.0",
    "gatsby-plugin-offline": "^6.0.0",
    "gatsby-plugin-manifest": "^5.0.0",
    "gatsby-plugin-mdx": "^5.13.0",
    "gatsby-plugin-postcss": "^6.0.0",
    "gatsby-plugin-robots-txt": "^1.8.0",
    "gatsby-plugin-sitemap": "^6.0.0",
    "gatsby-plugin-s3": "^0.3.8",
    "@zxing/browser": "^0.1.4",
    "gatsby-source-filesystem": "^5.13.0",
    "gray-matter": "^4.0.3",
    "leaflet": "^1.9.4",
    "oidc-client-ts": "^2.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-oidc-context": "^2.4.0",
    "pdfkit": "^0.15.0",
    "react-leaflet": "^4.2.1",
    "topojson-client": "^3.1.0",
    "us-atlas": "^3.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.12",
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.66",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.5.4"
  }
}
