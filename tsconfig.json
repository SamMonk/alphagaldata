{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "DOM",
      "ES2020"
    ],
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "module": "ESNext",
    "baseUrl": "."
  },
  "include": [
    "src/**/*",
    "gatsby-*.ts",
    "scripts/**/*"
  ]
}